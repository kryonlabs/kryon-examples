        Container {
            display: flex
            flex_direction: column
            align_items: start
            background_color: "#FFFFFFFF"
            border_radius: 8
            padding: 20
            gap: 12
            
            Text {
                text: "Button Styles"
                font_size: 18
                text_color: "#495057FF"
            }
            
            Container {
                display: flex
                flex_direction: row
                align_items: start
                gap: 12
                
                Button {
                    text: "Primary"
                    background_color: "#007BFFFF"
                    text_color: "#FFFFFFFF"
                    padding: 12
                    border_radius: 4
                }
                
                Button {
                    text: "Secondary"
                    background_color: "#6C757DFF"
                    text_color: "#FFFFFFFF"
                    padding: 12
                    border_radius: 4
                }
                
                Button {
                    text: "Success"
                    background_color: "#28A745FF"
                    text_color: "#FFFFFFFF"
                    padding: 12
                    border_radius: 4
                }
                
                Button {
                    text: "Danger"
                    background_color: "#DC3545FF"
                    text_color: "#FFFFFFFF"
                    padding: 12
                    border_radius: 4
                }
            }
        }
        
